{
  "name": "just-cook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix:other": "yarn prettier --write",
    "fix:code": "yarn test:code --fix",
    "fix": "yarn fix:code && yarn fix:other",
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "test": "yarn lint && jest --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.0.3",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
